pipeline{
    agent any
    stages{
        stage('hello'){
            steps{
                sh '''
                    echo this is first stage
                    '''
            }
        }
        stage('bye'){
            steps{
                sh '''
                    echo this is second stage
                    '''
            }
        }
        stage('final'){
            steps{
                sh '''
                    echo this is final stage
                    '''
            }
        }
    }
}