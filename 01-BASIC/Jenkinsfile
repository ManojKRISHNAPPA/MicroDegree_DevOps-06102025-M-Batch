pipeline{
    agent any

    stages{
        stage('Hello'){
            steps{
                sh'''
                    echo "First stage"
                '''
            }
        }

        stage('Middle-stage'){
            steps{
                sh'''
                    echo "middle stage"
                '''
            }
        }

        stage('Final-Stage'){
            steps{
                sh'''
                    echo "Final stage"
                '''
            }
        }

        stage('Docker build'){
            steps{
                sh'''
                    echo "this is just test"
                '''
            }

        }
        stage('container build'){
            steps{
                sh'''
                    echo "this is container test"
                '''
            }

        }
        stage('docker2 build'){
            steps{
                sh'''
                    echo "this is container test"
                '''
            }

        }
        stage('docker3 build'){
            steps{
                sh'''
                    echo "this is container test"
                '''
            }

        }
        
    }
}