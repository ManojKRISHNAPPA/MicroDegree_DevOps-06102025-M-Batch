pipeline
{
    agent none
        stages
    {
            stage("Dev Env")
            {
                parallel{
                    stage("build code"){
                        agent any
                        steps{
                            echo 'code review'
                        }
                    }
                     stage("test code"){
                        agent any
                        steps{
                            echo 'code review'
                        }
                    }
                }
            }
          stage("deploy code")
          {
                        agent any
                        steps{
                            echo 'deploy review'
                        }
          }
          stage("Testing Env")
            {
                parallel{
                    stage("manual tesing"){
                        agent any
                        steps{
                            echo 'code review'
                        }
                    }
                     stage("automation testing"){
                        agent any
                        steps{
                            echo 'code review'
                        }
                    }
                }
            }
            stage("production deployment")
          {
                        agent any
                        steps{
                            echo 'deploy review'
                        }
          }  
    }
}
        
    