pipeline{
    agent none
        stages{
            stage("Dev Env"){
                parallel{
                    stage("build code"){
                        agent any
                        steps{
                            echo 'code review'
                        }
                    }
                     stage("test code"){
                        agent any
                        steps{
                            echo 'code review'
                        }
                    }
                }
            }
            stage("test Env"){
                parallel{
                    stage("build code"){
                        agent any
                        steps{
                            echo 'code review'
                        }
                    }
                     stage("Automation"){
                        agent any
                        steps{
                            echo 'code review'
                        }
                    }
                }
            }
        }
    }