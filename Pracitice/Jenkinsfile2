pipeline{
    agent none
        stages{
            stage("Dev Env"){
                parallel{
                    stage("build code"){
                        agent any
                        steps{
                            echo 'code review'
                        }
                    stage("test  the code"){
                        agent any
                        steps{
                            echo "test the code"
                        }
                     }
                    }
                }
            }
        }
    
}