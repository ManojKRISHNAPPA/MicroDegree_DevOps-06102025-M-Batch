pipeline{
    agent any
        stages{
            stage("Dev Env"){
                parallel{
                    stage("build code"){
                        steps{
                            echo 'code review'
                        }
                    }
                     stage("test code"){
                        steps{
                            echo 'code review'
                        }
                    }
                }
            }
            stage("delpoy Env"){
                        steps{
                            echo 'code review'
                        }
                    }
                }
            }
            stage("test Env"){
                parallel{
                    stage("manual testing"){
                        steps{
                            echo 'code review'
                        }
                    }
                     stage("Automation"){
                        steps{
                            echo 'code review'
                        }
                    }
                }
            }
        }
    }